/**index.wxss**/
.slide-image{
  width:100%;
  height:150px;
}
.seach{
  padding:10px;
}
.navigator-hover{
    background: none;
}
.search-icon{
  margin-right:3px;
  position: relative;
  line-height: 25px;
  top:2px;
}
.search_box{
  border: 1px solid #ddd;
  border-radius:8px;
   text-align: center;
  font-size:14px;
  color:#aaa;
  line-height: 25px;
  height:25px;
}
.select_title{
  font-size:16px;
  color:#AB956D;
  padding-bottom:10px;
  text-align:center;
  padding-top:5px;
}
.select_top{
  overflow:hidden;
  height:165px;
  width:100%;
}
.select_item{
  flex:1;
  float:left;
  width:50%;
  overflow: hidden;
}
.select_item image{
  width:100%;
  height:165px;
}
.select_middle{
  height:150px;
}
.select_middle image{
  height:150px;
  width:100%;
}
.select_list{
  margin-top:10px;
}
.newset_box{
 overflow: hidden;
 margin-bottom:10px;
}
.newset_item{
  float:left;
  width:43%;
  margin-left:4%;
  background: #f5f6f5;
  text-align: center;
  border-radius: 10px;
  margin-top:10px;
}
.newset_item image{
  width:90%;
  height:100px;
}
.newset_item_title{
  font-size:14px;
  color:#AB956D;
}
.newset_item_price{
  font-size:13px;
  color:#AB956D;
}